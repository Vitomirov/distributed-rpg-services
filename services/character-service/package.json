{
  "name": "character-service",
  "version": "1.0.0",
  "private": true,
"scripts": {
  "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
  "build": "tsc",
  "start": "node dist/main.js",
  "typeorm": "npx typeorm-ts-node-commonjs",
  "migration:run": "npx typeorm-ts-node-commonjs migration:run -d src/config/db.ts",
  "migration:generate": "npx typeorm-ts-node-commonjs migration:generate -d src/config/db.ts"
},
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.28",
    "typeorm-ts-node-commonjs": "^0.3.20"
  },
  "devDependencies": {
    "@types/express": "^4.17.25",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
