{
  "name": "combat-service",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register src/main.ts",
    "start": "node -r tsconfig-paths/register dist/main.js",
    "build": "tsc",
    "test": "jest",
    "typeorm": "npx typeorm-ts-node-commonjs",
